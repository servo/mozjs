  rm -rf conftest*
  ac_cv_needs_atomic=no
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  _SAVE_LIBS="$LIBS"
                LIBS="$LIBS -latomic"
                cat > conftest.$ac_ext <<EOF
#line 3855 "configure"
#include "confdefs.h"
#include <cstdint>
                     #include <atomic>
int main() {
 std::atomic<uint64_t> foo; foo = 1; 
; return 0; }
