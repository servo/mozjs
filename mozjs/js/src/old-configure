if eval "test \"`echo '$''{'ac_cv_needs_atomic'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
                          if test "$CC_TYPE" = "clang" -a "$CPU_ARCH" = "x86" -a "$OS_ARCH" = "Linux"; then
            ac_cv_needs_atomic=yes
        else
            cat > conftest.$ac_ext <<EOF
#line 3837 "configure"
